<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_alumno">

    <fragment
        android:id="@+id/navigation_alumno"
        android:name="com.tecsup.pm.gocar.ui.alumno.AlumnoFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_alumno" />

    <fragment
        android:id="@+id/navigation_catalogo"
        android:name="com.tecsup.pm.gocar.ui.catalogo.CatalogoFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_catalogo" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.tecsup.pm.gocar.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

